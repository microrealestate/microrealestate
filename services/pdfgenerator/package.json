{
  "name": "pdfgenerator",
  "version": "1.0.0",
  "description": "Generates PDF documents (letters, contracts, invoices...)",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
